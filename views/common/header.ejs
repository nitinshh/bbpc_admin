    <!-- END: Custom CSS-->

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <% if (msg && (msg.includes('You are logged in successfully') ||
    msg.includes('Profile updated successfully') || msg.includes("Status changed successfully"))) { %>
    <script>
      document.addEventListener("DOMContentLoaded", function () {
        Swal.fire({
          position: "top-end",
          icon: "success",
          title: "<%= msg %>",
          showConfirmButton: false,
          timer: 3000,
          toast: true,
          customClass: {
            popup: "swal2-popup-custom",
            timerProgressBar: "swal2-timer-bar",
          },
        });
      });
    </script>
    <% } %>

    <style>
      .swal2-popup-custom {
        border-radius: 5px !important;
        padding: 13px 13px !important;
        font-family: "Roboto", sans-serif !important;
        font-size: 15px !important;
        letter-spacing: 0.5px !important;
        text-transform: capitalize !important;
      }
      .swal2-timer-bar {
        position: absolute;
        bottom: 0;
        left: 0;
        height: 3px;
        background: linear-gradient(to right, #ff8080, #cc4d4d);
        width: 100%;
        transition: width 0.3s linear;
      }
    </style>
  </head>
